{
  "name": "GitFiend",
  "version": "0.44.3",
  "description": "A Git client designed for humans",
  "homepage": "https://gitfiend.com",
  "main": "./resources/dist/main.js",
  "scripts": {
    "test": "jest --config ./jest.config.js --watch",
    "testAll": "jest --config ./jest.config.js",
    "postinstall": "ts-node ./scripts/override-electron-version.ts",
    "make": "npm run makeCore && webpack --progress --mode=production",
    "start": "electron resources/dist/main.js",
    "watch": "webpack --watch --progress --mode=development",
    "makeAskPass": "ts-node ./scripts/build-ask-pass.ts",
    "makeCore": "ts-node ./scripts/build-all-rust.ts",
    "watchInt": "webpack --watch --progress --mode=development --env testInt=true",
    "appx": "npm run make && electron-builder --config builder/appx.js",
    "dist": "npm run make && electron-builder --config builder/builder.config.js",
    "distDev": "npm run make && electron-builder --dir --config builder/builder.dev.config.js",
    "makeNative": "npm run makeCore && webpack --progress --mode=production --env macNative=true",
    "watchNative": "webpack --watch --progress --mode=development --env macNative=true",
    "watchServer": "webpack --watch --progress --mode=development --env server=true",
    "types": "ts-node ./scripts/generate-types.ts && prettier --write ./core-types"
  },
  "author": "Toby Suggate",
  "license": "MIT",
  "devDependencies": {
    "@electron/notarize": "^2.3.0",
    "@tauri-apps/api": "^1.5.3",
    "@types/chai": "^4.3.14",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/webpack-dev-server": "^4.7.2",
    "chai": "^5.1.0",
    "dotenv": "^16.4.5",
    "electron": "28.3.1",
    "electron-builder": "^24.13.3",
    "electron-window-state": "^5.0.3",
    "esbuild-loader": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "ifdef-loader": "^2.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mocha": "^10.4.0",
    "node-fetch": "^2.6.9",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
