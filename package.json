{
  "name": "GitFiend",
  "version": "0.44.3",
  "description": "A Git client designed for humans",
  "homepage": "https://gitfiend.com",
  "main": "./resources/dist/main.js",
  "scripts": {
    "test": "jest --config ./jest.config.js --watch",
    "testAll": "jest --config ./jest.config.js",
    "postinstall": "tsx ./scripts/override-electron-version.ts",
    "make": "npm run makeCore && webpack --progress --mode=production",
    "start": "electron resources/dist/main.js",
    "startDev": "electron resources-dev/dist/main.js",
    "watch": "tsx ./scripts/watch.ts",
    "watchShell": "webpack --watch --progress --mode=development",
    "watchDev": "webpack --watch --progress --mode=development --env devResources=true",
    "makeAskPass": "tsx ./scripts/build-ask-pass.ts",
    "makeCore": "tsx ./scripts/build-all-rust.ts",
    "appx": "npm run make && electron-builder --config builder/appx.js",
    "dist": "npm run make && electron-builder --config builder/builder.config.js",
    "distDev": "npm run make && electron-builder --dir --config builder/builder.dev.config.js"
  },
  "author": "Toby Suggate",
  "license": "MIT",
  "devDependencies": {
    "@electron/notarize": "^2.3.2",
    "@types/jest": "^29.5.12",
    "dotenv": "^16.4.5",
    "electron": "^29.4.2",
    "electron-builder": "^24.13.3",
    "electron-window-state": "^5.0.3",
    "esbuild-loader": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.2",
    "tsx": "^4.15.4",
    "typescript": "^5.4.5",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  }
}
